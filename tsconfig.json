{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es5",
			"es2015.promise",
			"dom"
		],
		"baseUrl": ".",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": true
	},
	"exclude": [
		"bower_components",
		"node_modules"
	]
}